# Agent700 API Configuration
# Copy this file to .env and fill in your actual values

# Your Agent700 account email
EMAIL="your-email@company.com"

# Your Agent700 account password
PASSWORD="your_secure_password"

# The unique identifier for the Agent700 agent
AGENT_UUID="your-agent-uuid-here"

# Optional: API Base URL (defaults to production)
# API_BASE_URL="https://api.agent700.ai"
# For local development: API_BASE_URL="http://localhost:5601"
# For staging: API_BASE_URL="https://staging-api.agent700.ai"
